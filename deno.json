{
  "name": "@deno/loader",
  "tasks": {
    "wasmbuild": "cd src/rs_lib && deno run -A jsr:@deno/wasmbuild@0.19.2 --out ../lib"
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  },
  "publish": {
    "exclude": [
      ".github",
      ".vscode",
      ".gitmodules",
      "deno.lock",
      "!./src/lib",
      "scripts",
      "tests/"
    ]
  },
  "exports": "./src/mod.ts",
  "exclude": [
    "deno",
    "src/rs_lib/target",
    "target",
    "tests/**/testdata"
  ],
  "imports": {
    "@david/dax": "jsr:@david/dax@^0.43.2",
    "@std/assert": "jsr:@std/assert@^1.0.13",
    "@std/toml": "jsr:@std/toml@^1.0.7"
  }
}
